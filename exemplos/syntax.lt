fn square(x: int): int {
    return x * x;
}

-- this is a comment.
fn main(String[] args): void {
    let x, i: int;

    print(Oi, digite um numero: );
    scan(x);

    for [i = 0; i < x; i = i + 1] {
        if [i > 0 && true] {
            println(square(i));

            if [i < 5] {
                println(square(i) * square(i));
            }
        else {
            println(It's getting big!);
        }
    }

    while [true] {
        break;
    }
}
